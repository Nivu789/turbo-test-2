{
    "version": 2,
    "builds": [
      {
        "src": "apps/web/package.json",
        "use": "@vercel/next"
      },
      {
        "src": "apps/docs/package.json",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/docs(.*)",
        "dest": "/apps/docs$1"
      },
      {
        "src": "/(.*)",
        "dest": "/apps/web$1"
      }
    ]
  }